@model IEnumerable<ArtCatalog.Models.Category>
<p>
    @Html.ActionLink("Добавить новую категорию", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.CategoryID }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.CategoryID })
        </td>
    </tr>
}

</table>
